AAXA,CAAA","sourcesContent":["<template>\n  <div class=\"m-5\">\n    \n    <v-col cols=\"12\" sm=\"4\" class=\"grey lighten-4 mx-5\">\n      <h3>Bookmark an image</h3>  \n\n      <v-form @submit.prevent=\"createImage\">\n        <v-text-field\n          v-model=\"image.title\"\n          label=\"Title\"\n          required\n        ></v-text-field>\n        <v-text-field\n          v-model=\"image.description\"\n          label=\"Description\"\n          required\n        ></v-text-field>\n        <v-text-field\n          v-model=\"image.url\"\n          label=\"Url\"\n          required\n        ></v-text-field>\n        <v-btn class=\"mr-4\" color=\"success\" type=\"submit\">Create image</v-btn>\n      </v-form>      \n\n    </v-col>\n  </div>\n</template>\n\n\n<script>\n\n  export default {\n    name: \"ImageCreate\",\n    data() {\n      return {\n        image: {\n          'name': '',\n          'description': '',\n          'url': '',\n        }\n      }\n    },\n    methods: {\n      async createImage() {\n        let token = localStorage.getItem('token');\n        let requestOptions = {\n          method: 'POST',\n          headers: {\n            \"Content-type\": 'application/json',\n            \"Authorization\": \"JWT \" + token,\n          },\n          body: JSON.stringify(this.image)\n        }\n        var response = await fetch(\"http://127.0.0.1:8000/images/api-create/\", requestOptions);\n        this.image.push(await response.json());\n      }\n    }\n  }\n</script>"],"sourceRoot":"src/views"}]}